node N {
	nodeSize.constraints: NODE_LABELS
	
	label "The name of this glorious node" {
		nodeLabels.placement: "INSIDE V_TOP H_CENTER"
	}
	
	label "Left bottom" {
		nodeLabels.placement: "INSIDE V_BOTTOM H_LEFT"
	}
	label "432Hz" {
		nodeLabels.placement: "INSIDE V_BOTTOM H_LEFT"
	}
	
	label "Right bottom" {
		nodeLabels.placement: "INSIDE V_BOTTOM H_RIGHT"
	}
	label "440Hz" {
		nodeLabels.placement: "INSIDE V_BOTTOM H_RIGHT"
	}
}