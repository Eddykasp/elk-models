/* Tests north south ports. N1 should be connected to a northern port of N3,
 * N2 to a southern port of N3.
 */

node N1 {
	layout [ size: 100, 75 ]
	portConstraints: FIXED_POS
	label L1: "N1"
	port a {
		layout [
			position: 100, 20
			size: 10, 10
		]
	}
}
node N2 {
	layout [ size: 100, 75 ]
	portConstraints: FIXED_POS
	label L2: "N2"
	port b {
		layout [
			position: 100, 20
			size: 10, 10
		]
	}
}
node N3 {
	layout [ size: 100, 75 ]
	portConstraints: FIXED_POS
	label L3: "N3"
	port c {
		layout [
			position: 50, -10
			size: 10, 10
		]
	}
	port d {
		layout [
			position: 50, 75
			size: 10, 10
		]
	}
}
edge E1: N1.a -> N3.c
edge E2: N2.b -> N3.d
