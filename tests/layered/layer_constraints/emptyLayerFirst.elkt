/**
 * Test case for KIPRA-1561
 */
 
// as our testing framework does not support recursive layout we 
// explicitly create dummy nodes that would be created for external ports

node extDummyFirst {
	layerConstraint: FIRST_SEPARATE
}
node first {
	layerConstraint: FIRST
}
node n
node n2
node extDummyLast {
	layerConstraint: LAST_SEPARATE
}
edge E1: extDummyFirst -> first
edge E2: first -> n
edge E3: n -> n2
edge E4: n2 -> extDummyLast
