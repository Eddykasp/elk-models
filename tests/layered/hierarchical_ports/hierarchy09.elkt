/* Compound graph with a compound node with a more complex configuration of
 * north and south ports. If port constraints of the compound node are set
 * to fixed side, this diagram can be drawn without edge crossings. If they
 * are set to FIXED_ORDER, edge crossings cannot be avoided.
 */
node N1 {
	layout [ size: 50, 50 ]
	label L1: "N1"
}
node N2 {
	layout [ size: 50, 50 ]
	label L2: "N2"
}
node N3 {
	layout [ size: 50, 50 ]
	label L3: "N3"
}
node Compound {
	layout [ size: 100, 20 ]
	edgeRouting: ORTHOGONAL
	portConstraints: FIXED_SIDE
	port CN1 {
		layout [
			position: 10, -5
			size: 5, 5
		]
	}
	port CN2 {
		layout [
			position: 50, -5
			size: 5, 5
		]
	}
	port CW1 {
		layout [
			position: -5, 10
			size: 5, 5
		]
	}
	port CE1 {
		layout [
			position: 100, 10
			size: 5, 5
		]
	}
	node NC1 {
		layout [ size: 50, 50 ]
		label L4: "NC1"
	}
	node NC2 {
		layout [ size: 50, 50 ]
		label L5: "NC2"
	}
	node NC3 {
		layout [ size: 50, 50 ]
		label L6: "NC3"
	}
	node NC4 {
		layout [ size: 50, 50 ]
		label L7: "NC4"
	}
	edge E5: NC1 -> NC2
	edge E6: NC1 -> CN2
	edge E7: NC2 -> CN2
	edge E8: NC3 -> CN1
}
edge E1: N1 -> Compound.CN1
edge E2: N1 -> Compound.CN2
edge E3: N2 -> Compound.CW1
edge E4: Compound.CE1 -> N3
