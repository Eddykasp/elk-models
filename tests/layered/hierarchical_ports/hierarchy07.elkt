/* Compound graph with a simple compound node with hierarchical ports.
 */
node Compound {
	edgeRouting: ORTHOGONAL
	portConstraints: FIXED_SIDE
	port CN1 {
		layout [ size: 5, 5 ]
		side: NORTH
	}
	port CW1 {
		layout [ size: 5, 5 ]
		side: WEST
	}
	node NC1 {
		layout [ size: 50, 50 ]
		label L1: "NC1"
	}
	node NC2 {
		layout [ size: 50, 50 ]
		label L2: "NC2"
	}
	edge E1: CW1 -> NC1
	edge E2: NC2 -> CN1
}
