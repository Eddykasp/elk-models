/* has multiple which should "reach into" each other */

fixedAlignment: LEFTUP
node c1_n1 {
	layout [ size: 50, 50 ]
	label L1: "c1_n1"
}
node c1_n2 {
	layout [ size: 50, 50 ]
	label L2: "c1_n2"
}
node c1_n3 {
	layout [ size: 50, 100 ]
	label L3: "c1_n3"
}
node c2_n1 {
	layout [ size: 50, 100 ]
	label L4: "c2_n1"
}
node c2_n2 {
	layout [ size: 50, 50 ]
	label L5: "c2_n2"
}
node c3_n1 {
	layout [ size: 50, 50 ]
	label L6: "c3_n1"
}
node c4_n1 {
	layout [ size: 50, 50 ]
	label L7: "c4_n1"
}
edge E1: c1_n1 -> c1_n2
edge E2: c1_n2 -> c1_n3
edge E3: c2_n1 -> c2_n2
edge E4: c2_n1 -> c1_n3
edge E5: c3_n1 -> c1_n2
edge E6: c4_n1 -> c1_n2
