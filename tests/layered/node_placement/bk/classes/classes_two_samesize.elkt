/*
 * This test checks that separations between classes are propagated properly 
 * between classes that depend on each other.
 * 
 * For the traversal direction: RIGHTDOWN
 * 
 * The sinks are: b0_1, b4_1, b5_2
 */
separateConnectedComponents: false
fixedAlignment: RIGHTDOWN
crossingMinimization.strategy: INTERACTIVE
layering.strategy: INTERACTIVE

/*  
 * Class1  
 */
 
node lu_b0_1 {
	layout [ size: 50, 50 ]
	label L1: "c1_b0_1"
}
node lu_b0_2 {
	layout [
		position: 100, 0
		size: 50, 50
	]
	label L2: "c1_b0_2"
}
node lu_b1_1 {
	layout [
		position: 100, 100
		size: 50, 50
	]
	label L3: "c1_b1_1"
}
node lu_b1_2 {
	layout [
		position: 200, 100
		size: 50, 50
	]
	label L4: "c1_b1_2"
}

/*  
 * Class2
 */
 
node lu_b3_1 {
	layout [
		position: 200, 0
		size: 50, 50
	]
	label L5: "c2_b3_1"
}
node lu_b3_2 {
	layout [
		position: 300, 0
		size: 50, 50
	]
	label L6: "c2_b3_2"
}
node lu_b4_1 {
	layout [
		position: 200, -100
		size: 50, 50
	]
	label L7: "c2_b4_1"
}

/*  
 * Class3  
 */
 
node lu_b5_1 {
	layout [
		position: 300, -100
		size: 50, 50
	]
	label L8: "c3_b5_1"
}
node lu_b5_2 {
	layout [
		position: 300, -200
		size: 50, 50
	]
	label L9: "c3_b5_2"
}
edge E1: lu_b0_1 -> lu_b0_2
edge E2: lu_b1_1 -> lu_b1_2
edge E3: lu_b3_1 -> lu_b3_2
