/* Tests inverted ports features with four nodes and a feedback edge. Node
 * positions are set such that the interactive cycle breaker reverses the
 * edge from N4 back to N1.
 */
cycleBreaking.strategy: INTERACTIVE
node N1 {
	layout [ size: 50, 50 ]
	nodeLabels.placement: "[H_CENTER, V_CENTER, INSIDE]"
	portConstraints: FIXED_SIDE
	label L1: "N1"
	port P1_1 {
		layout [ size: 5, 5 ]
		side: WEST
	}
	port P1_2 {
		layout [ size: 5, 5 ]
		side: EAST
	}
}
node N2 {
	layout [
		position: 100, 0
		size: 50, 50
	]
	nodeLabels.placement: "[H_CENTER, V_CENTER, INSIDE]"
	portConstraints: FIXED_SIDE
	label L2: "N2"
	port P2_1 {
		layout [ size: 5, 5 ]
		side: WEST
	}
	port P2_2 {
		layout [ size: 5, 5 ]
		side: EAST
	}
}
node N3 {
	layout [
		position: 200, 0
		size: 50, 50
	]
	nodeLabels.placement: "[H_CENTER, V_CENTER, INSIDE]"
	portConstraints: FIXED_SIDE
	label L3: "N3"
	port P3_1 {
		layout [ size: 5, 5 ]
		side: WEST
	}
	port P3_2 {
		layout [ size: 5, 5 ]
		side: EAST
	}
}
node N4 {
	layout [
		position: 300, 0
		size: 50, 50
	]
	nodeLabels.placement: "[H_CENTER, V_CENTER, INSIDE]"
	portConstraints: FIXED_SIDE
	label L4: "N4"
	port P4_1 {
		layout [ size: 5, 5 ]
		side: WEST
	}
	port P4_2 {
		layout [ size: 5, 5 ]
		side: EAST
	}
}
edge E1: N1.P1_2 -> N2.P2_1
edge E2: N2.P2_2 -> N3.P3_1
edge E3: N3.P3_2 -> N4.P4_1
edge E4: N4.P4_2 -> N1.P1_1
