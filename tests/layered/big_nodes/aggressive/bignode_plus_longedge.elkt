algorithm: layered
layering.wideNodesOnMultipleLayers: AGGRESSIVE

node n1 {
	layout [ size: 100, 100 ]
	label L1: "n1"
}
node n2 {
	layout [ size: 100, 100 ]
	label L2: "n2"
}
node n3 {
	layout [ size: 100, 100 ]
	label L3: "n3"
}
node n4 {
	layout [ size: 100, 100 ]
	label L4: "n4"
}
node n5 {
	layout [ size: 100, 100 ]
	label L5: "n5"
}
node n6 {
	layout [ size: 100, 100 ]
	label L6: "n6"
}
node bigone {
	layout [ size: 300, 100 ]
	label L7: "bigone"
}
edge E1: n1 -> n2 {
	layout [
		section ES1 []
	]
}
edge E2: n1 -> bigone {
	layout [
		section ES2 []
	]
}
edge E3: n2 -> n3 {
	layout [
		section ES3 []
	]
}
edge E4: n3 -> n4 {
	layout [
		section ES4 []
	]
}
edge E5: n4 -> n5 {
	layout [
		section ES5 []
	]
}
edge E6: n5 -> n6 {
	layout [
		section ES6 []
	]
}
edge E7: bigone -> n6 {
	layout [
		section ES7 []
	]
}
edge E8: bigone -> n6 {
	layout [
		section ES8 []
	]
}
