// the example illustrates how big nodes processing can introduce edge/node overlaps

algorithm: layered
layering.wideNodesOnMultipleLayers: AGGRESSIVE

node n0 {
	layout [ size: 50, 50 ]
	label L1: "n0"
}
node n1 {
	layout [ size: 150, 50 ]
	label L2: "n1"
}
node n2 {
	layout [ size: 50, 50 ]
	label L3: "n2"
}
node n3 {
	layout [ size: 100, 50 ]
	label L4: "n3"
}
node n4 {
	layout [ size: 200, 50 ]
	label L5: "n4"
}
node n5 {
	layout [ size: 200, 50 ]
	label L6: "n5"
}
node n6 {
	layout [ size: 150, 50 ]
	label L7: "n6"
}
node n7 {
	layout [ size: 50, 50 ]
	label L8: "n7"
}
edge E1: n0 -> n1
edge E2: n0 -> n2
edge E3: n0 -> n3
edge E4: n1 -> n6
edge E5: n1 -> n7
edge E6: n2 -> n4
edge E7: n2 -> n5
edge E8: n2 -> n6
edge E9: n3 -> n5
edge E10: n3 -> n6
edge E11: n3 -> n7
edge E12: n4 -> n7
