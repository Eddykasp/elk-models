algorithm: layered
layering.wideNodesOnMultipleLayers: CAREFUL

node n1 {
	layout [ size: 50, 50 ]
	portConstraints: FIXED_SIDE
	label L1: "n1"
	port p11 {
		layout [ size: 10, 10 ]
		side: EAST
	}
	port p12 {
		layout [ size: 10, 10 ]
		side: EAST
	}
}
node n2 {
	layout [ size: 50, 50 ]
	portConstraints: FIXED_SIDE
	label L2: "n2"
	port p21 {
		layout [ size: 10, 10 ]
		side: EAST
	}
	port p22 {
		layout [ size: 10, 10 ]
		side: WEST
	}
}
node n3 {
	layout [ size: 50, 50 ]
	portConstraints: FIXED_SIDE
	label L3: "n3"
	port p31 {
		layout [ size: 10, 10 ]
		side: EAST
	}
	port p32 {
		layout [ size: 10, 10 ]
		side: WEST
	}
}
node n4 {
	layout [ size: 50, 50 ]
	portConstraints: FIXED_SIDE
	label L4: "n4"
	port p41 {
		layout [ size: 10, 10 ]
		side: WEST
	}
	port p42 {
		layout [ size: 10, 10 ]
		side: WEST
	}
}
node bignode {
	layout [ size: 280, 50 ]
	portConstraints: FIXED_SIDE
	label L5: "bignode2222222222"
	port b1 {
		layout [
			position: -10, 20
			size: 10, 10
		]
	}
	port b2 {
		layout [ size: 10, 10 ]
		side: EAST
	}
}
edge E1: n1.p11 -> n2.p22
edge E2: n1.p12 -> bignode.b1
edge E3: n2.p21 -> n3.p32
edge E4: n3.p31 -> n4.p42
