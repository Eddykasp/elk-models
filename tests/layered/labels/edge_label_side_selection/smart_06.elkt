// The label should be under the edge. There was a bug in the initial implementation
// of smart label side selection that caused it not to be.
algorithm: layered
elk.direction: RIGHT
edgeLabels.sideSelection: SMART_DOWN

node N0
node N1

edge N0 -> N1 { label "An excellent edge label" }
