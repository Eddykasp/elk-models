/* Compound graph with three connected components and a few hierarchical ports
 * with free port constraints.
 */
node Compound {
	edgeRouting: ORTHOGONAL
	portConstraints: FREE
	port CW1 {
		layout [ size: 5, 5 ]
	}
	port CW2 {
		layout [ size: 5, 5 ]
	}
	port CE1 {
		layout [ size: 5, 5 ]
	}
	node N1_1 {
		layout [ size: 50, 50 ]
		label L1: "N1_1"
	}
	node N1_2 {
		layout [ size: 50, 50 ]
		label L2: "N1_2"
	}
	node N2_1 {
		layout [ size: 50, 50 ]
		label L3: "N2_1"
	}
	node N2_2 {
		layout [ size: 50, 50 ]
		label L4: "N2_2"
	}
	node N2_3 {
		layout [ size: 50, 50 ]
		label L5: "N2_3"
	}
	node N2_4 {
		layout [ size: 50, 50 ]
		label L6: "N2_4"
	}
	node N2_5 {
		layout [ size: 50, 50 ]
		label L7: "N2_5"
	}
	node N3_1 {
		layout [ size: 50, 50 ]
		label L8: "N3_1"
	}
	edge E1: CW1 -> N2_1
	edge E2: CW2 -> N1_1
	edge E3: N1_1 -> N1_2
	edge E4: N2_1 -> N2_2
	edge E5: N2_2 -> N2_3
	edge E6: N2_3 -> N2_4
	edge E7: N2_4 -> N2_5
	edge E8: N2_5 -> CE1
}
