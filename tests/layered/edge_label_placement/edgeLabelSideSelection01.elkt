/* Tests the edge label side selection strategy mechanism thingy. In this
 * diagram, all labels should be placed above their respective edge.
 */
 
sideSelection: ALWAYS_UP
debugMode: true
edgeRouting: ORTHOGONAL
node N1
node N2
node N3
node N4
edge E1: N1 -> N2 {
	label L1: "A cheese walks into a bar..."
}
edge E2: N2 -> N3 {
	label L2: "...and the bar man says:" {
		edgeLabels.placement: TAIL
	}
}
edge E3: N2 -> N4 {
	label L3: "\'Sorry, we don\'t serve any food here.\'" {
		edgeLabels.placement: HEAD
	}
}
