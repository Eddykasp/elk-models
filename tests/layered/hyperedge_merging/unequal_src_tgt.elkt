node n1 {
	portConstraints: FIXED_SIDE
	port e { ^port.side: EAST }
}
node n2 {
	portConstraints: FIXED_SIDE
	port w { ^port.side: WEST }
	port e { ^port.side: EAST }
}
node n3 {
	portConstraints: FIXED_SIDE
	port w { ^port.side: WEST }
	port e { ^port.side: EAST }
}
node n4 {
	portConstraints: FIXED_SIDE
	port w { ^port.side: WEST }
}

edge n1.e -> n2.w
edge n1.e -> n4.w
edge n3.e -> n4.w
edge n2.w -> n3.e  
edge n2.e -> n3.w    	 