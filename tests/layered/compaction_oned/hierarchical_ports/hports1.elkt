node parent {
	
	portLabels.placement: INSIDE
	separateConnectedComponents: false
	
	port w1
	port w2
	
	port e1 { label "a"}
	port e2
	
	
	node n1
	node n2
	
	edge parent.w1 -> n1 { label "hugo" }
	edge n1 -> parent.e1

	edge parent.w2 -> n2
	edge n2 -> parent.e2 { label "oguh"}
	
	
}
  